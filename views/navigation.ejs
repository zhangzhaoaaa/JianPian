

<script>
    $(function(){
        $('.nav-sidebar li').click(function(){
            var view=$(this);
            $.ajax({
                url: $(this).attr('data-url'),
                dataType:'html',
                success:function(data){
                    //$('#mainContent').remove().append(data);
                    view.addClass('active').siblings().removeClass('active');
                }
            });
        });
    })
</script>